    </Card>\n        <br>\n        <br>\n        <Card :bordered=\"false\">\n          <div slot=\"title\" style=\"margin-bottom: 32px\">\n            <label for=\"select_lauguage\" style=\"float: left;margin-top: 7px;font-size: 15px\">Language:</label>\n            <Select v-model=\"select_language\" style=\"width:200px;float: left;\" id=\"select_lauguage\" @on-change=\"onLanguageChange(select_language)\">\n              <Option v-for=\"item in languageList\" :value=\"item.value\" :key=\"item.value\">{{ item.label }}</Option>\n            </Select>\n            <Button ghost type=\"warning\" style=\"float: left; margin-left: 5px\" @click.native=\"reset_modal = true\">\n              <Icon type=\"md-refresh\"/>\n            </Button>\n          </div>\n          <div slot=\"extra\">\n            <label for=\"select_theme\" style=\"margin-top: 7px;font-size: 15px\">Theme:</label>\n            <Select v-model=\"select_theme\" style=\"width:200px\" id=\"select_theme\" @on-change=\"onThemeChange(select_theme)\">\n              <Option v-for=\"item in themeList\" :value=\"item.value\" :key=\"item.value\">{{ item.label }}</Option>\n            </Select>\n          </div>\n          <div>\n            <label for=\"code_content\" style=\"display: none\"></label>\n            <textarea id=\"code_content\" name=\"code_content\" style=\"display: none\"></textarea>\n          </div>\n          <Divider/>\n          <Row>\n            <div>\n              <Button ghost type=\"primary\" size=\"large\" style=\"float: right;margin-right: 50px\" @click.native=\"submitCode\" :disabled=\"loginStatus\">\n                <Icon type=\"md-create\"/>\n                Submit\n              </Button>\n            </div>\n          </Row>\n        </Card>\n      </Col>\n      <Col span=\"3\">\n        <Card @click.native=\"updateActiveName\">\n          <router-link to=\"/status\">\n            <div style=\"float: left;\">\n              <Icon type=\"md-code-working\" size=\"30\"/>\n         